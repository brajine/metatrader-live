{
    "swagger": "2.0",
    "info": {
        "description": "Swagger API doc for Metatrader.live.",
        "title": "Metatrader.live API",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/api/rest/{page}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Provide actual data on connected account",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Account Page name",
                        "name": "page",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/metatrader.Account"
                        }
                    }
                }
            }
        },
        "/api/stats": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Provide actual list of connected accounts",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/metatrader.StateData"
                        }
                    }
                }
            }
        },
        "/api/wss/{page}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Provide actual data on connected account via WebSocket connection",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Account Page name",
                        "name": "page",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/metatrader.Account"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "metatrader.Account": {
            "type": "object",
